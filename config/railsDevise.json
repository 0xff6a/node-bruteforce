{
  "captureOptions": {
    "csrfRegex": "/<input name=\"authenticity_token\" type=\"hidden\" value=\"(.{44})\" \/>/",
    "loginRegex": [
      "/Invalid email or password/"
    ]
  },
  "csrfRequest": {
    "url": "%URL%",
    "method": "GET",
    "headers": {},
    "rejectUnauthorized": false
  },
  "loginRequest": {
    "path": "%URL%",
    "method": "POST",
    "headers": {
      "Cookie": "%COOKIE%"
    },
    "form": {
      "authenticity_token": "%TOKEN%",
      "user[email]": "%USER%",
      "user[password]": "%PASS%",
      "commit": "Sign+in"
    },
    "rejectUnauthorized": false
  }
}