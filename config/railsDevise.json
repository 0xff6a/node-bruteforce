{
  "capture": {
    "csrfRegex": "<input name=\"authenticity_token\" type=\"hidden\" value=\"(.{44})\"",
    "loginRegex": [
      "Invalid email or password"
    ]
  },
  "csrf": {
    "url": "%URL%",
    "method": "GET",
    "headers": {},
    "rejectUnauthorized": false
  },
  "login": {
    "url": "%URL%",
    "method": "POST",
    "headers": {
      "Cookie": "%COOKIE%"
    },
    "form": {
      "authenticity_token": "%TOKEN%",
      "user[email]": "%USER%",
      "user[password]": "%PASS%",
      "commit": "Sign+in"
    },
    "rejectUnauthorized": false
  }
}