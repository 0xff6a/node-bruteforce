{
  "captureOptions": {
    "csrfRegex": "//",
    "loginRegex": []
  },
  "csrfRequest": {
    "url": "^URL^",
    "method": "GET",
    "headers": {},
    "rejectUnauthorized": false
  },
  "loginRequest": {
    "path": "^URL^",
    "method": "POST",
    "headers": {
      "Cookie": "^COOKIE^"
    },
    "form": {
      "authenticity_token": "^TOKEN^",
      "user": "^USER^",
      "password": "^PASS^",
    },
    "rejectUnauthorized": false
  }
}