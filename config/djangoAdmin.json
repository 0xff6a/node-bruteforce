{
  "captureOptions": {
    "csrfRegex": "/<input type='hidden' name='csrfmiddlewaretoken' value='(.*)' \/>/",
    "loginRegex": [
      "/Please enter the correct username/",
      "/Please correct the error below/"
    ]
  },
  "csrfRequest": {
    "url": "%URL%",
    "method": "GET",
    "headers": {},
    "rejectUnauthorized": false
  },
  "loginRequest": {
    "path": "%URL%",
    "method": "POST",
    "headers": {
      "Referer": "%URL%",
      "Cookie": "csrftoken=%TOKEN%"
    },
    "form": {
      "csrfmiddlewaretoken": "%TOKEN%",
      "username": "%USER%",
      "password": "%PASS%",
      "next": "/admin/"
    },
    "rejectUnauthorized": false
  }
}