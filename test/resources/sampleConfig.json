{
  "capture": {
    "csrfRegex": "token=(.*)",
    "loginRegex": [
      "bad login"
    ]
  },
  "csrf": {
    "url": "%URL%/csrf",
    "method": "GET",
    "headers": {},
    "rejectUnauthorized": false
  },
  "login": {
    "url": "%URL%/login",
    "method": "POST",
    "headers": {
      "Cookie": "%COOKIE%"
    },
    "form": {
      "authenticity_token": "%TOKEN%",
      "user": "%USER%",
      "password": "%PASS%"
    },
    "rejectUnauthorized": false
  }
}